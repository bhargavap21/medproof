{
  "name": "@medproof/circuits",
  "version": "1.0.0",
  "description": "Zero-knowledge circuits for medical statistics verification",
  "main": "index.js",
  "scripts": {
    "compile": "circom circuits/medical_stats.circom --r1cs --wasm --sym",
    "compile:all": "npm run compile:stats && npm run compile:range && npm run compile:significance",
    "compile:stats": "circom circuits/medical_stats.circom --r1cs --wasm --sym -o build/",
    "compile:range": "circom circuits/range_proof.circom --r1cs --wasm --sym -o build/",
    "compile:significance": "circom circuits/statistical_significance.circom --r1cs --wasm --sym -o build/",
    "setup": "node scripts/setup.js",
    "generate-proof": "node scripts/generate_proof.js",
    "verify-proof": "node scripts/verify_proof.js"
  },
  "devDependencies": {
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.0"
  },
  "dependencies": {
    "ffjavascript": "^0.2.60"
  },
  "keywords": ["zk-proofs", "circom", "medical-research", "privacy"],
  "author": "MedProof Team",
  "license": "MIT"
}